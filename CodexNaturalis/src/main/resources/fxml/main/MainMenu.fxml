<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="mainPane" prefHeight="1080.0" prefWidth="1920.0" maxHeight="1080" maxWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <VBox id="mainVbox" alignment="CENTER_LEFT" AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0">
        <Button mnemonicParsing="false" onAction="#goChooseConnection" text="Play"/>
        <Button mnemonicParsing="false" onAction="#goOptions" text="Options"/>
        <Button mnemonicParsing="false" onAction="#goRules" text="Rules"/>
        <Button mnemonicParsing="false" onAction="#goCredits" text="Credits"/>
        <Button mnemonicParsing="false" onAction="#exit" text="Exit"/>
    </VBox>
</AnchorPane>
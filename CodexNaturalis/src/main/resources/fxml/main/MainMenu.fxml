<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="mainPane" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <VBox id="mainVbox" alignment="CENTER_LEFT" prefHeight="1080" prefWidth="400">
        <Button mnemonicParsing="false" onAction="#goChooseConnection" text="Play"/>
        <Button mnemonicParsing="false" onAction="#goOptions" text="Options"/>
        <Button mnemonicParsing="false" text="Rules"/>
        <Button mnemonicParsing="false" onAction="#goCredits" text="Credits"/>
        <Button mnemonicParsing="false" onAction="#exit" text="Exit"/>
    </VBox>
</AnchorPane>
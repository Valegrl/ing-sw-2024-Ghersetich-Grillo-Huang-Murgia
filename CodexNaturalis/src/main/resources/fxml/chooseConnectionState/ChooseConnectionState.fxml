<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane id="mainPane" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
       <VBox alignment="CENTER" prefWidth="1920" prefHeight="1080" spacing="60">
               <VBox alignment="CENTER" prefWidth="1920" spacing="20">
                   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose your connection type" textAlignment="CENTER" wrappingWidth="500.0" />
                   <Button alignment="CENTER" onAction="#setSocket" mnemonicParsing="false" text="Socket" textAlignment="CENTER" />
                   <Button alignment="CENTER" onAction="#setRmi" mnemonicParsing="false" text="RMI" textAlignment="CENTER" />
               </VBox>
           <Button alignment="CENTER" onAction="#goBackMain" mnemonicParsing="false" text="Back" textAlignment="CENTER" />
       </VBox>
</AnchorPane>
